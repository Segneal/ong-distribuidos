# URL del API Gateway
REACT_APP_API_URL=http://localhost:3000/api

# GraphQL endpoint configuration
REACT_APP_GRAPHQL_URL=http://localhost:3000/api/graphql

# Configuración de desarrollo
REACT_APP_ENV=development

# Configuración de logging
REACT_APP_LOG_LEVEL=info

# Configuración de la organización
REACT_APP_ORG_NAME=Empuje Comunitario
REACT_APP_ORG_ID=empuje-comunitario

# Configuración de features (para desarrollo)
REACT_APP_ENABLE_NETWORK_FEATURES=false
REACT_APP_ENABLE_DEBUG_MODE=true